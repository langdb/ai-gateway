<clickhouse>
  <logger>
    <level>warning</level>
    <console>true</console>
  </logger>
  <!-- Configuration of user defined executable functions -->
  <user_defined_executable_functions_config>/etc/clickhouse-server/config.d/functions/*_functions.*ml</user_defined_executable_functions_config>
  <user_scripts_path>/var/lib/clickhouse_scripts/user_scripts/</user_scripts_path>
</clickhouse>